<body class="container">
    <div style="  background: #d5d4d7;;padding:20px;">
        <div class="intro">
            <div style="background-color:rgb(5, 18, 122)"> 
            <h1>It's Popping!</h1>
            </div>
            <div class="vibeImg">     
           <img src="/icon.png" style="width: 320px;height:200px"/>
            </div>
        </div>
       <div class="row">
        {{#each featured}}
            <div class="featured col-sm" style="margin: 10px;margin-top: 30px;margin-bottom: 30px;">
                <img src={{this.img}}class="img-rounded" alt="poster"/>
                <h3>{{this.name}}</h3>
                <h4>{{this.fee}}</h4>
                <p>{{this.description}}</p>
            </div>
        {{/each}}
      </div>
    </div>
</body>