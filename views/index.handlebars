<body class="container">
    <div style="background: #e9e9e9;padding:20px">
        <div class="intro">
            <h1>It's Popping!</h1>
        </div>
       <div class="row">
        {{#each featured}}
            <div class="featured col-sm" style="margin: 5px;">
                <img src={{this.img}} alt="poster"/>
                <h3>{{this.name}}</h3>
                <h4>{{this.fee}}</h4>
                <p>{{this.description}}</p>
            </div>
        {{/each}}
      </div>
    </div>
</body>